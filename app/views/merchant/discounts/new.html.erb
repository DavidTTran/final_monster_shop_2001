<h1>Create Discount</h1>
<center>
  <%= form_tag "/merchant/discounts", method: :post do %>
    <%= label_tag :quantity %>
    <%= text_field_tag :quantity%>

    <%= label_tag :percentage %>
    <%= text_field_tag :percentage %>
<%= submit_tag 'Create Discount' %>
  <% end %>
  <br><%= link_to "Go Back", request.referer %>
</center>
